{
	"name": "aeris/zf-di-config",
	"description": "Configurable dependency injection for the ZF2 ServiceManager",
	"keywords": [
		"ZF2",
		"Zend Framework 2",
        "ServiceManager",
        "Dependency Injection"
	],
	"homepage": "http://www.aerisweather.com/",
	"require": {
		"php": ">=5.3.3",
		"zendframework/zendframework": "2.3.*"
	},
	"require-dev": {
		"mockery/mockery": "~0.9",
        "phpunit/phpunit": "~3.7"
	},
	"autoload": {
		"psr-4": {
			"Aeris\\ZfDiConfig\\": "src/ZfDiConfig/",
            "Aeris\\ZfDiConfigTest\\": "test/ZfDiConfigTest/"
		},
		"classmap": [
            "src/Module.php"
        ]
	},
	"minimum-stability1": "dev"
}
